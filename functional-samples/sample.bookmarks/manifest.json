{
  "name": "Bookmark Sync With Notion",
  "description": "[Alice TODO] A browser action with a popup dump of all bookmarks, including search, add, edit and delete.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "bookmarks",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_title": "Bookmark Sync With Notion",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background":{
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/bookmark-16.png",
    "32": "images/bookmark-32.png",
    "48": "images/bookmark-48.png",
    "128": "images/bookmark-128.png"
  }
}